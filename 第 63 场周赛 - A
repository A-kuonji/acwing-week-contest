#include <iostream>
#include <cstring>
#include <algorithm>

using namespace std;

int main()
{
    int a, b, c; cin >> a >> b >> c;
    int res = 0;
    for(int i = 0; i <= a; i ++ ) {
        for(int j = 0; j <= b; j ++ ) {
            if(i + j == c) res++;
        }
    }
    
    cout << res << endl;
    return 0;
}
